{
  "manifest_version": 3,
  "name": "Video Speed Regulator",
  "description": "Adjust your video playback rate",
  "version": "1.0.1",
  "background": {
    "service_worker": "./js/sw.js"
  },
  "options_ui": {
    "page": "options.html"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "action": {
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
