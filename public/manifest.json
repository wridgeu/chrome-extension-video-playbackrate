{
	"manifest_version": 3,

	"name": "Getting Started with Chrome Extensions",
	"description": "trying to build a simple extension",
	"version": "1.0",

	"background": {
		"service_worker": "js/sw.js"
	},

	"options_ui": {
		"page": "options.html"
	},

	"permissions": ["storage", "activeTab", "scripting"],

	"action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},

	"icons": "icon.png"
}