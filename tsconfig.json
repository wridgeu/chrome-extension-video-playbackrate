{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "rootDir": "./src",
    "outDir": "./dist/js/",
    "importHelpers": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "noEmitOnError": true,
    "moduleResolution": "Node",
    "typeRoots": ["./node_modules/@types", "./types/index.d.ts"]
  }
}
